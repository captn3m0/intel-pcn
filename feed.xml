---
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0"
	xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
>
	<channel>
		<title>{{site.title}}</title>
		<description>{{site.description}} All new PCNs are published on this feed daily.</description>		
		<sy:updatePeriod>daily</sy:updatePeriod>
		<sy:updateFrequency>1</sy:updateFrequency>
		<link>{{ site.url }}{{site.baseurl}}</link>
		<atom:link href="{{ site.url }}{{site.baseurl}}feed.xml" rel="self" type="application/rss+xml" />
		<lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
		{% assign D = site.data.pcn | sort:"createddate" | reverse %}			
		{% for pcn in D limit:10 %}
			<item>
				<title>{{ pcn.systitle |  xml_escape }}</title>
				<description>{%capture description%}

					<strong>PCN ID: </strong> {{pcn.contentid}}
					<br>
					{{pcn.abstract}}
					<br>
					Complete PCN PDF is available at <a href="{{ pcn.sysuri}}">{{ pcn.sysuri}}</a>.
					<strong>Description: </strong>{{pcn.description}}<br>
					<strong>Type: </strong>{{pcn.downloadtype}}<br>
					<strong>Intel HTML: </strong><a href="https://www.intel.com{{pcn.secondaryurl}}">Intel Website</a><br>


					{%endcapture%}

					{{ description | xml_escape }}</description>
				<pubDate>{{ pcn.createddate|divided_by:1000 | date_to_rfc822 }}</pubDate>
				<link>{{ site.url }}{{site.baseurl}}pcn/{{pcn.contentid}}/</link>
				<guid>PCN-{{pcn.id}}</guid>
			</item>
		{% endfor %}
	</channel>
</rss>